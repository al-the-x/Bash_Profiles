#!/usr/bin/env python
import sys

words = sys.argv; words.pop(0)

for word in words:
    try:
        try:
            __import__(word)
        except ImportError: # not a module...
            module, _ = word.rsplit('.', 1)
            __import__(module)

        help(word)
    except ImportError:
        print('Cannot find "%s" to import!' % word)

